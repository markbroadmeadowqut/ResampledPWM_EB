#include "DSP28x_Project.h"
#include "config.h"

#include <math.h>

Uint16 Table[1024];




#pragma CODE_SECTION(serviceDDS, "ramfuncs");
Uint16 serviceDDS() {
	phaseAccumulator += phaseInc;

	return ddsTable[(phaseAccumulator>>22)&0x3FF];	// For 10-bit table, top 10 bits of accumulator are index
}


